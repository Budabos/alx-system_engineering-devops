frontend http-in
    bind *:80
    redirect scheme https code 301 if !{ ssl_fc }

frontend https-in
    bind *:443 ssl crt /etc/ssl/private/budabos.tech.pem
    default_backend servers

backend servers
    balance roundrobin
    server 168448-web-01 54.84.65.229:80 check
    server 168448-web-02 54.175.134.97:80 check
